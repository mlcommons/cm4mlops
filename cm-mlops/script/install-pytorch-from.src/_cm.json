{
  "alias": "install-pytorch-from.src",
  "automation_alias": "script",
  "automation_uid": "5b4e0237da074764",
  "cache": true,
  "category": "Compiler automation",
  "deps": [
    {
      "tags": "detect,os"
    },
    {
      "tags": "detect,cpu"
    },
    {
      "tags": "get,python3",
      "names": [
        "python",
        "python3"
      ]
    },
    {
      "tags": "install,llvm,src,_tag.llvmorg-15.0.7,_runtimes.libcxx:libcxxabi:openmp,_clang,_release"
    },
    {
      "env": {
        "CM_GIT_CHECKOUT_PATH_ENV_NAME": "CM_PYTORCH_SRC_REPO_PATH"
      },
      "extra_cache_tags": "pytorch,src,pytorch-src,pytorch-src-repo",
      "names": [
        "pytorch-src-repo"
      ],
      "tags": "get,git,repo,_full-history",
      "update_tags_from_env_with_prefix": {
        "_branch.": [
          "CM_GIT_CHECKOUT"
        ],
        "_repo.": [
          "CM_GIT_URL"
        ],
        "_sha.": [
          "CM_GIT_CHECKOUT_SHA"
        ],
        "_tag.": [
          "CM_GIT_CHECKOUT_TAG"
        ]
      }
    }
  ],
  "env": {
    "CM_GIT_URL": "https://github.com/pytorch/pytorch"
  },
  "name": "Build pytorch from sources",
  "new_env_keys": [
    "CM_PYTORCH_*",
    "CM_GET_DEPENDENT_CACHED_PATH"
  ],
  "post_deps": [
    {
      "skip_if_env": {
        "CM_REQUIRE_INSTALL": [
          "yes"
        ]
      },
      "tags": "get,llvm"
    }
  ],
  "prehook_deps": [],
  "sort": 1000,
  "tags": [
    "install",
    "src",
    "from.src",
    "pytorch",
    "src-pytorch"
  ],
  "uid": "64eaf3e81de94f41",
  "variations": {
    "branch.#": {
      "env": {
        "CM_GIT_CHECKOUT": "#"
      }
    },
    "repo.https://github.com/pytorch/pytorch": {
      "env": {
        "CM_GIT_URL": "https://github.com/pytorch/pytorch"
      },
      "group": "repo",
      "default": true 
    },
    "repo.#": {
      "env": {
        "CM_GIT_URL": "#"
      },
      "group": "repo"
    },
    "sha.#": {
      "env": {
        "CM_GIT_CHECKOUT_SHA": "#"
      }
    },
    "tag.#": {
      "env": {
        "CM_GIT_CHECKOUT_TAG": "#"
      }
    }
  },
  "versions": {}
}
