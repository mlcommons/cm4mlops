{
  "alias": "get-tvm",
  "automation_alias": "script",
  "automation_uid": "5b4e0237da074764",
  "deps": [
    {
      "tags": "set,echo-off,win"
    },
    {
      "tags": "cmake,get-cmake",
      "version_min": "3.18"
    },
    {
      "tags": "detect,cpu"
    }
  ],
  "extra_cache_tags_from_env": [
    {"env":"CM_LLVM_CACHE_TAGS", "prefix":"llvm-"}
  ],
  "env": {
    "CM_GIT_CHECKOUT": "main",
    "CM_GIT_URL": "https://github.com/apache/tvm"
  },
  "new_env_keys": [
    "TVM_HOME",
    "+PYTHONPATH"
  ],
  "cache": true,
  "tags": [
    "get",
    "tvm",
    "get-tvm"
  ],
  "uid": "93c89140e6224f4b",
  "variations": {
    "cuda": {
      "deps": [
        {
          "tags": "lib,cuda"
        }
      ],
      "env": {
        "CM_USE_CUDA": "yes"
      }
    },
    "llvm": {
      "env": {
        "CM_TVM_USE_LLVM": "yes"
      },
      "deps": [
        {
          "tags": "get,llvm"
        }
      ]
    },
    "openmp": {
      "env": {
        "CM_TVM_USE_OPENMP": "yes"
      }
    }
  },
  "versions": {
    "main": {
      "env": {
        "CM_GIT_CHECKOUT": "main"
      }
    },
    "v0.7.0": {
      "env": {
        "CM_GIT_CHECKOUT": "v0.7.0"
      }
    },
    "v0.8.0": {
      "env": {
        "CM_GIT_CHECKOUT": "v0.8.0"
      }
    },
    "v0.9.0": {
      "env": {
        "CM_GIT_CHECKOUT": "v0.9.0"
      }
    }
  }
}
