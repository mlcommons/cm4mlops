{
  "alias": "get-mlperf-inference-src",
  "automation_alias": "script",
  "automation_uid": "5b4e0237da074764",
  "category": "MLPerf benchmark support",
  "cache": true,
  "default_version": "master",
  "deps": [
    {
      "tags": "detect,os"
    },
    {
      "names": [
        "python",
        "python3"
      ],
      "tags": "get,python3"
    }
  ],
  "prehook_deps": [
    {
      "tags": "get,git,repo",
      "update_tags_from_env_with_prefix": {
        "_repo.": [ "CM_GIT_URL" ],
        "_branch.": [ "CM_GIT_CHECKOUT" ],
        "_sha.": [ "CM_GIT_SHA" ],
        "_submodules.": [ "CM_GIT_SUBMODULES" ]
      },
      "force_env_keys": [
        "CM_GIT_*"
      ],
      "names": [
        "inference-git-repo"
      ],
      "env": {
        "CM_GIT_CHECKOUT_PATH_ENV_NAME": "CM_MLPERF_INFERENCE_SOURCE"
      },
      "extra_cache_tags": "inference,src"
    }
  ],
  "default_env": {
    "CM_GIT_DEPTH": "--depth 4",
    "CM_GIT_PATCH": "no",
    "CM_GIT_URL": "https://github.com/mlcommons/inference.git",
    "CM_GIT_RECURSE_SUBMODULES": "",
    "CM_GIT_CHECKOUT_FOLDER": "inference"
  },
  "new_env_keys": [
    "CM_MLPERF_INFERENCE_3DUNET_PATH",
    "CM_MLPERF_INFERENCE_BERT_PATH",
    "CM_MLPERF_INFERENCE_CLASSIFICATION_AND_DETECTION_PATH",
    "CM_MLPERF_INFERENCE_CONF_PATH",
    "CM_MLPERF_INFERENCE_DLRM_PATH",
    "CM_MLPERF_INFERENCE_DLRM_V2_PATH",
    "CM_MLPERF_INFERENCE_GPTJ_PATH",
    "CM_MLPERF_INFERENCE_RNNT_PATH",
    "CM_MLPERF_INFERENCE_SOURCE",
    "CM_MLPERF_INFERENCE_VERSION",
    "CM_MLPERF_INFERENCE_VISION_PATH",
    "CM_MLPERF_LAST_RELEASE",
    "+PYTHONPATH"
  ],
  "tags": [
    "get",
    "src",
    "source",
    "inference",
    "inference-src",
    "inference-source",
    "mlperf",
    "mlcommons"
  ],
  "uid": "4b57186581024797",
  "variations": {
    "full-history": {
      "group": "git-history",
      "env": {
        "CM_GIT_DEPTH": ""
      }
    },
    "octoml": {
      "base": [
        "short-history"
      ],
      "env": {
        "CM_GIT_URL": "https://github.com/octoml/inference"
      }
    },
    "patch": {
      "env": {
        "CM_GIT_PATCH": "yes"
      },
      "ad": {
        "inference-git-repo": {
          "tags": "_patch"
        }
      }
    },
    "nvidia-pycocotools": {
      "env": {
        "CM_GIT_PATCH_FILENAME": "coco.patch"
      },
      "base": [
        "patch"
      ]
    },
    "openimages-nvidia-pycocotools": {
      "env": {
        "CM_GIT_PATCH_FILENAME": "openimages-pycocotools.patch"
      },
      "base": [
        "patch"
      ]
    },
    "deepsparse": {
      "env": {
        "CM_GIT_URL": "https://github.com/neuralmagic/inference",
        "CM_GIT_CHECKOUT": "deepsparse",
        "CM_MLPERF_LAST_RELEASE": "v3.0"
      }
    },
    "no-recurse-submodules": {
      "env": {
        "CM_GIT_RECURSE_SUBMODULES": ""
      }
    },
    "recurse-submodules": {
      "env": {
        "CM_GIT_RECURSE_SUBMODULES": " --recurse-submodules"
      }
    },
    "short-history": {
      "group": "git-history",
      "default": true,
      "env": {
        "CM_GIT_DEPTH": "--depth 10"
      }
    },
    "pybind": {
      "env": {
        "CM_SUBMODULE_PYBIND": "yes"
      }
    },
    "deeplearningexamples": {
      "env": {
        "CM_SUBMODULE_DEEPLEARNINGEXAMPLES": "yes"
      }
    },
    "3d-unet": {
      "env": {
        "CM_SUBMODULE_3D_UNET": "yes"
      }
    },
    "gn": {
      "env": {
        "CM_SUBMODULE_GN": "yes"
      }
    },
    "repo.#": {
      "env": {
         "CM_GIT_URL" : "#"
      }
    },
    "branch.#": {
      "group": "checkout",
      "env": {
        "CM_GIT_CHECKOUT": "#"
      }
    },
    "sha.#": {
      "group": "checkout",
      "env": {
        "CM_GIT_SHA": "#"
      }
    },
    "submodules.#": {
      "env": {
        "CM_GIT_SUBMODULES": "#"
      }
    }
  },
  "versions": {
    "master": {
      "env": {
        "CM_MLPERF_LAST_RELEASE": "v4.0",
        "CM_GIT_CHECKOUT": "master"
      }
    },
    "r3.1": {
      "env": {
        "CM_MLPERF_LAST_RELEASE": "v3.1",
        "CM_GIT_CHECKOUT": ""
      },
      "adr": {
        "inference-git-repo": {
          "tags": "_tag.v3.1"
        }
      }
    },
    "r3.0": {
      "env": {
        "CM_MLPERF_LAST_RELEASE": "v3.0",
        "CM_GIT_CHECKOUT": ""
      },
      "adr": {
        "inference-git-repo": {
          "tags": "_tag.v3.0"
        }
      }
    },
    "r2.1": {
      "env": {
        "CM_MLPERF_LAST_RELEASE": "v2.1",
        "CM_GIT_CHECKOUT": "v2.1"
      }
    },
    "tvm": {
      "env": {
        "CM_GIT_URL": "https://github.com/mlcommons/inference",
        "CM_GIT_CHECKOUT": "tvm",
        "CM_MLPERF_LAST_RELEASE": "v3.1"
      }
    },
    "deepsparse": {
      "env": {
        "CM_GIT_URL": "https://github.com/neuralmagic/inference",
        "CM_GIT_CHECKOUT": "deepsparse",
        "CM_MLPERF_LAST_RELEASE": "v3.1"
      }
    },
    "custom": {
      "env": {
        "CM_MLPERF_LAST_RELEASE": "v3.1"
      }
    },
    "pybind_fix": {
      "env": {
        "CM_MLPERF_LAST_RELEASE": "v3.1",
        "CM_GIT_URL": "https://github.com/GATEOVerflow/inference",
        "CM_GIT_CHECKOUT": "pybind_update"
      }
    }
  },
  "print_env_at_the_end" : {
    "CM_MLPERF_INFERENCE_SOURCE": "Path to MLPerf inference benchmark sources",
    "CM_MLPERF_INFERENCE_CONF_PATH": "Path to the MLPerf inference benchmark configuration file"
  }
}
