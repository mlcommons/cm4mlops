    "onnx,fp32,debug": {
      "env": {
        "CM_ML_MODEL_ACCURACY": "0.8107",
        "CM_PACKAGE_URL": "https://dlrm.s3-us-west-1.amazonaws.com/models/tb0875_10M.onnx.tar",
        "CM_ML_MODEL_DLRM_MAX_INDEX_RANGE": "10000000",
        "CM_UNTAR": "yes",
        "CM_ML_MODEL_FILE": "tb0875_10M.onnx"
      }
    }
  }
