{
  "alias": "get-python-lib",
  "automation_alias": "script",
  "automation_uid": "5b4e0237da074764",
  "cache": true,
  "clean_files": [],
  "deps": [
    {
      "tags": "set,echo-off,win"
    },
    {
      "tags": "detect-cpu"
    },
    {
      "names": [
        "python",
        "python3"
      ],
      "tags": "get,python3"
    }
  ],
  "extra_cache_tags_from_env": [
    {
      "env": "CM_PYTHON_CACHE_TAGS",
      "prefix": "python-"
    }
  ],
  "new_env_keys": [
    "CM_PYTHONLIB_*",
    "CM_*_VERSION"
  ],
  "prehook_deps": [
    {
      "enable_if_env": {
        "CM_TMP_REQUIRE_INSTALL": [
          "yes"
        ]
      },
      "names": [ "python-lib-install" ],
      "reuse_version": true,
      "inherit_variation_tags": true,
      "tags": "install,python-lib,generic"
    }
  ],
  "tags": [
    "get",
    "generic",
    "python-lib"
  ],
  "uid": "94b62a682bc44791",
  "variations": {
    "onnxruntime": {
      "env": {
        "CM_PYTHON_PACKAGE_NAME": "onnxruntime"
      },
      "adr": {
        "python-lib-install": {
          "tags": "_onnxruntime"
        }
      }
    },
    "tensorflow": {
      "env": {
        "CM_PYTHON_PACKAGE_NAME": "tensorflow"
      },
      "adr": {
        "python-lib-install": {
          "tags": "_tensorflow"
        }
      }
    }
  }
}
